# Configuration de l'environnement MyCard
# Copier ce fichier vers .env et remplir avec vos valeurs

# Firebase Configuration
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_API_KEY=your_api_key
FIREBASE_APP_ID=your_app_id
FIREBASE_MESSAGING_SENDER_ID=your_sender_id

# Build Configuration
MYCARD_KEYSTORE_PATH=/path/to/your/keystore.jks
MYCARD_KEYSTORE_PASSWORD=your_keystore_password
MYCARD_KEY_ALIAS=your_key_alias
MYCARD_KEY_PASSWORD=your_key_password

# Environment
APP_ENV=development

